<h1>Listing articles</h1>

<table>
  <thead>
    <tr>
      <th>Headline</th>
      <th>Image</th>
    </tr>
  </thead>

  <tbody>
    <% @article_metadata.each do |article| %>
      <tr>
        <td><%= article[:headline] %></td>
        <td><%= article[:image] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
